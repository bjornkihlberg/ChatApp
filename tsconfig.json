{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "alwaysStrict": true,
        "removeComments": true,
        "jsx": "react",
    },
    "exclude": [
        "node_modules"
    ]
}